<div class="container">
  <section class="header">
    <h1 class="title">X Marks Your Start</h1>


    <h4 id="conceptionRange"></h4>

    <div class="value-props row">
      <div class="four columns value-prop">
        <%= fa_icon "calendar-times-o 4x" %>
        <h3>Events</h3>
      </div>
      <div class="four columns value-prop">
        <%= fa_icon "headphones 4x" %>
        <h3>Songs</h3>
      </div>
      <div class="four columns value-prop">
        <%= fa_icon "film 4x" %>
        <h3>Movies</h3>
      </div>
    </div>
    <div class="value-props row">

      <div class="four columns value-prop">
        <p id="events"></p>
      </div>

      <div class="four columns value-prop">
        <p id="songs"></p>
      </div>

      <div class="four columns value-prop">
        <p id="movies"></p>
      </div>
    </div>

    <button id="birthdayInputButton" href="http://www.xmarksyourstart.com">Find out about your own conception</button>
  </section>
  <hr>
</div>
