<meta property="og:url"           content="http://www.xmarksyourstart.com" />
<meta property="og:type"          content="website" />
<meta property="og:title"         content="X Marks Your Start" />
<meta property="og:description"   content="Find out what influenced your conception" />
<meta property="og:image" content = "http://failedmessiah.typepad.com/.a/6a00d83451b71f69e20168ebc80e7c970c-800wi" />

<div class="container">
  <section class="header">
    <%= image_tag "new_logo_2.png" %>
    <h4 id="conceptionRange_user"></h4>

    <div class="value-props row">
      <div class="four columns value-prop">
        <%= fa_icon "calendar-times-o 4x" %>
        <h3>Events</h3>
      </div>
      <div class="four columns value-prop">
        <%= fa_icon "headphones 4x" %>
        <h3>Songs</h3>
      </div>
      <div class="four columns value-prop">
        <%= fa_icon "film 4x" %>
        <h3>Movies</h3>
      </div>
    </div>
    <div class="value-props row">

      <div class="four columns value-prop">
        <p id="events"></p>
      </div>

      <div class="four columns value-prop">
        <p id="songs"></p>
      </div>

      <div class="four columns value-prop">
        <p id="movies"></p>
      </div>
    </div>

    <a class="button home-button" href="https://www.xmarksyourstart.com">Find out about your own conception</a>
  </section>
  <hr>
</div>
